{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "shared": "./shared",
            "static": "./static"
          }
        }
      ],
      ["babel-plugin-styled-components", { "ssr": true, "displayName": true, "preprocess": false }]
    ]
  }